# Documize API Configuration
# Copy this file to .env and fill in your credentials

# Your Documize instance URL (e.g., https://docs.example.com)
DOCUMIZE_API_URL=

# Your Documize credentials (Base64 encoded "orgId:email:password")
# Generate with: echo -n "your-org-id:your-email:your-password" | base64
# Example: echo -n "demo:api@example.org:test" | base64
DOCUMIZE_API_CREDENTIALS=
